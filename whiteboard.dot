digraph whiteboard{

rankdir=BT

node [ shape = "record" ]

#master interface
Quant [label="{Quant | Name(): string \l Unit(): string \l NComp(): int \l IGet(comp, index): float32}"]

#subinterfaces
Scalar [label="{Scalar | IGet1(index): float32}"]
Scalar->Quant

Vector [label="{Vector | IGet3(index): [3]float32}"]
Vector->Quant

Uniform [label="{Uniform | Get(comp): float32}"]
Uniform->Quant

UniformScalar [label="{UniformScalar | Get1(): float32}"]
UniformScalar->Quant

UniformVector [label="{UniformVector | Get3(): [3]float32}"]
UniformVector->Quant

rank=same{Scalar; Vector; Uniform; UniformScalar; UniformVector}

#concrete
UniformN [label="{UniformN}"]
UniformN->Uniform

Uniform1 [label="{Uniform1}"]
Uniform1->Uniform
Uniform1->Scalar
Uniform1->UniformScalar

Uniform3 [label="{Uniform3}"]
Uniform3->Uniform
Uniform3->Vector
Uniform3->UniformVector

List [label="{List}"]
List->Quant

ScalarList [label="{ScalarList}"]
ScalarList->Scalar
ScalarList->List

VectorList [label="{VectorList}"]
VectorList->Vector 
VectorList->List

rank=same{Uniform1; UniformN; Uniform3; List; VectorList; ScalarList}
}
